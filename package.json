{
	"name": "alpha-dxtionary",
	"displayName": "alpha-dxtionary",
	"description": "",
	"version": "0.0.1",
	"author": {
		"name": "Hong-Phuc"
	},
	"publisher": "XXXX",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/hpb-htw/alpha-dxtionary.git"
	},
	"activationEvents": [
		"onCommand:alpha.dxtionary.lookup",
		"onCommand:alpha.dxtionary.lookup.ui",
		"onCommand:alpha.dxtionary.lookup.cursor",
		"onCommand:alpha.dxtionary.extract.builtin.dict"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "alpha.dxtionary.lookup.ui",
				"title": "alpha dxtionary manual lookup"
			},
			{
				"command": "alpha.dxtionary.lookup.cursor",
				"title": "alpha dxtionary lookup word under cursor"
			},
			{
				"command": "alpha.dxtionary.extract.builtin.dict",
				"title": "alpha dxtionary extract built-in dictionary"
			}
		],
		"keybindings": [
			{
				"command": "alpha.dxtionary.lookup.cursor",
				"key": "ctrl+e",
				"mac": "cmd+e",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"vscode:public": "./scripts/release.sh",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"clean": "rm -rf out *.vsix",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"unit:test": "node --inspect --trace-uncaught ./node_modules/.bin/mocha --ui tdd --require ts-node/register --watch-extensions ts,tsx \"./src/unittest/**/*.{ts,tsx}\"",
		"refresh": "rm -rf node_modules out; npm install --no-optional",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/lodash": "^4.14.149",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"cssnano": "^4.1.10",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"ts-node": "^8.5.4",
		"tslint": "^5.20.0",
		"typescript": "^3.8.2",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"lodash": "^4.17.15",
		"wikinary-eintopf": "file://../wikinary-eintopf",
		"de-wiktionary-parser": "file://../de-wiktionary-parser"
	}
}
